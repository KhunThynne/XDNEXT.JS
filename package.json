{
  "name": "xd_nextjs",
  "version": "1.1.8",
  "description": "NextJS develop-thynne",
  "scripts": {
    "dev:frontend": "npm --prefix frontend/xdstudio run dev",
    "dev:backend": "npm --prefix backend/xdstudio  run dev",
    "prisma:studio": "npm --prefix backend/xdstudio run prisma:studio",
    "dev": "concurrently \"npm run dev:frontend/xdstudio\" \"npm run dev:backend\"",
    "build:frontend": "npm --prefix frontend/xdstudio  run build",
    "build:backend": "npm --prefix backend/xdstudio  run build",
    "build": "npm run build:frontend/xdstudio && npm run build:backend/xdstudio ",
    "prepare": "husky",
   "release": "npx standard-version --skip.commit --skip.tag && git push --follow-tags origin develop-thynne"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhunThynne/XDNEXT.JS.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/KhunThynne/XDNEXT.JS/issues"
  },
  "homepage": "https://github.com/KhunThynne/XDNEXT.JS#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "releaseRules": [
      {
        "type": "chore",
        "release": false
      },
      {
        "type": "docs",
        "release": false
      },
      {
        "type": "style",
        "release": false
      },
      {
        "type": "test",
        "release": false
      },
      {
        "type": "refactor",
        "release": "patch"
      },
      {
        "type": "perf",
        "release": "patch"
      },
      {
        "type": "fix",
        "release": "patch"
      },
      {
        "type": "feat",
        "release": "minor"
      }
    ]
  }
}
