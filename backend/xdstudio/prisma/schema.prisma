generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String?  
  username      String?
  provider  String   @default("credentials")

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
