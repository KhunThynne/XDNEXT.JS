  role                         String?            @default("USER")
  email                        String             @unique @default("")
  password                     String
  posts                        Post[]             @relation("Post_author")
  createdAt                    DateTime?          @default(now())
  passwordResetToken           String?
  passwordResetIssuedAt        DateTime?
  passwordResetRedeemedAt      DateTime?
  from_UserProduct_userId      UserProduct[]      @relation("UserProduct_userId")
  from_UserPreference_userId   UserPreference[]   @relation("UserPreference_userId")
  from_UserPoint_userId        UserPoint[]        @relation("UserPoint_userId")
