}

model Product {
  id                         String        @id @default(uuid()) @postgresql.Uuid
  suppilersId                Suppiler?     @relation("Product_suppilersId", fields: [suppilersIdId], references: [id])
  suppilersIdId              String?       @map("suppilersId") @postgresql.Uuid
