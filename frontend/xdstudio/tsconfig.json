{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      },
      {
        "name": "@0no-co/graphqlsp",
        "schema": "./src/libs/graphql/graphql.schema.graphql"
      }
    ],
    "paths": {
      "@navigation": ["./src/libs/i18n/navigation.ts"],
      "@configs/*": ["./configs/*"],
      "@type/*": ["./types/*"],
      "@public/*": ["./public/*"],
      // "#/*": ["./*"],
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/env.ts",
    "src/types/**/*.d.ts",
    ".next\\dev/types/**/*.ts",
    ".next\\dev/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": ["node_modules"],
  "typeRoots": ["./types", "./node_modules/@types"],
  "types": ["next-auth"]
}
